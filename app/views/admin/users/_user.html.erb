
<tbody>
  <tr>
    <td>
      <%= user.id %>
    </td>
    <td>
      <%= user.name   %>
    </td>
    <td>
      <%= user.email   %>
    </td>
    <td>
      <%= user.phone   %>
    </td>
    <td>
      <%= user.address  %>
    </td>
    <td>
      <%= user.human_gender   %>
    </td>
    <td>
      <%= user.birthday   %>
    </td>
    <td>
      <%= link_to admin_user_path(user.id), method: :delete, style:"color: #cecece;", data: { confirm: 'Are you sure?' }  do %>
        <i class="fas fa-trash-alt"></i>
        <% end %>
    </td>
    <td>
      <%= link_to edit_admin_user_path(user.id), style:"color: #cecece;" do %>
        <i class="fa-solid fa-pen"></i>
        <% end %>
    </td>

  </tr>
</tbody>
