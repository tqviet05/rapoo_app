
<tbody>
  <tr>
    <td>
      <%= user.id %>
    </td>
    <td>
      <%= user.name.blank? ? 'No value': user.name   %>
    </td>
    <td>
      <%= user.email.blank? ? 'No value': user.email   %>
    </td>
    <td>
      <%= user.phone.blank? ? 'No value': user.phone   %>
    </td>
    <td>
      <%= user.address.blank? ? 'No value': user.address  %>
    </td>
    <td>
      <%= user.gender   %>
    </td>
    <td>
      <%= user.birthday.blank? ? 'No value': user.birthday   %>
    </td>
    <td>
      <%= link_to admin_user_path(user.id), method: :delete, style:"color: #cecece;" do %>
        <i class="fas fa-trash-alt"></i>
        <% end %>
    </td>
    <td>
      <%= link_to edit_admin_user_path(user.id), style:"color: #cecece;" do %>
        <i class="fa-solid fa-pen"></i>
        <% end %>
    </td>

  </tr>
</tbody>
